{
    "name": "laltu/laravel-roles-menu",
    "description": "laravel roles menu",
    "keywords": [
        "laltu",
        "laravel-roles-menu"
    ],
    "homepage": "https://github.com/laltu/laravel-roles-menu",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "laltu das",
            "email": "laltu.lspl@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Laltu\\LaravelRolesMenu\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Laltu\\LaravelRolesMenu\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Laltu\\LaravelRolesMenu\\LaravelRolesMenuServiceProvider"
            ],
            "aliases": {
                "LaravelRolesMenu": "Laltu\\LaravelRolesMenu\\LaravelRolesMenuFacade"
            }
        }
    }
}
